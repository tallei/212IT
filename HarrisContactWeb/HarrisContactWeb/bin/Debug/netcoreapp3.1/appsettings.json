{ // connection string is a configuration information in DbContext taht support in database connection 
  "ConnectionStrings": { // open bracket of connection string for local SQL database or local DB
    "DefaultConnection": "Server=(localDb)\\mssqllocaldb;Database=HarrisContactDb;Trusted_Connection=True;MultipleActiveResultSets=True" 
  }, // multiple active result to enable multiple users access the program same time 
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
